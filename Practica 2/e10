10.A)
HAND EQU 40H

ORG 1000H
CADENA DB "INGENIERIA E INFORMATICA"
FIN_ DB ?

ORG 2000H
;CONFIGURO HANDSHAKE
MOV AL,0
OUT HAND+1,AL;DESACTIVO INTERRUPCIONES
MOV BX,OFFSET CADENA
MOV CL,OFFSET FIN_ - OFFSET CADENA
;ESPERO SI ESTA BUSY
LOOP:
PUSH AX
IN AL,HAND+1
AND AL,1
JNZ LOOP
;IMPRIMO
MOV AL,[BX]
OUT HAND,AL
INC BX
DEC CL
JNZ LOOP
HLT
END
